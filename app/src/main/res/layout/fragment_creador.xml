<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/CoordinadorLayout"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context="es.esy.pruebaappluis.cityme.Activities_Fragments.FragmentCreador">

    <!-- Inicio de la TabLayout-->
    <android.support.design.widget.TabLayout
        android:id="@+id/TabLayoutCreador"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        style="@style/tabLayoutStyleCreador"/>
    <!-- Fin de la TabLayout-->

    <FrameLayout
        android:id="@+id/content_creador"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</android.support.design.widget.CoordinatorLayout>
